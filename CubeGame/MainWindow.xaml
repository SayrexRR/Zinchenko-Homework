<Window x:Class="CubeGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CubeGame"
        mc:Ignorable="d"
        Title="Cube Game" Height="450" Width="800" KeyDown="Window_KeyDown" Closing="Window_Closing">
    <Canvas>
        <Rectangle x:Name="square" Fill="Blue" Width="50" Height="50" Canvas.Left="50" Canvas.Top="50"/>
    </Canvas>
</Window>
